{
  "@jupyterlab/apputils-extension:themes": {
      "theme": "JupyterLab Dark"
  },
  "@jupyterlab/shortcuts-extension:shortcuts": {
      "shortcuts": [
      {
          "command": "imageviewer:flip-horizontal",
          "keys": [
              "X"
          ],
          "selector": ".jp-ImageViewer"
      },
      {
        "command": "imageviewer:flip-horizontal",
        "keys": [
            "H"
        ],
        "selector": ".jp-ImageViewer",
        "disabled": true
    }
    ]
  }
}